    "express": "^4.14.0",
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-css-modules": "^4.7.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-socket.io": "^1.4.0",
    "redux-thunk": "^2.2.0",
