{
  "name": "redux-universal-tic-tac-toe",
  "version": "0.0.1",
  "description": "Multiplayer SPA game",
  "main": "server/bin/server.js",
  "scripts": {
    "dev": "npx webpack-dev-server --config config/client.dev.config.js",
    "dev:server": "NODE_ENV=development npm run build:server && node server/bin/server.js",
    "build": "rimraf server/bin && npm run build:client && npm run build:server",
    "build:server": "NODE_ENV=production webpack --config config/server.config.js --progress",
    "build:client": "NODE_ENV=production webpack --config config/client.prod.config.js --progress",
    "lint": "npx eslint --ext .jsx,.js ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shvedovskiy/redux-universal-tic-tac-toe"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "expressjs",
    "react router v4",
    "redux",
    "socket.io",
    "universal",
    "multiplayer",
    "game"
  ],
  "author": {
    "name": "Oleg Shvedov",
    "email": "ol.shvedov@gmail.com",
    "url": "https://github.com/shvedovskiy/redux-universal-tic-tac-toe"
  },
  "license": "SEE LICENCE IN LICENSE",
  "bugs": {
    "url": "https://github.com/shvedovskiy/redux-universal-tic-tac-toe/issues",
    "email": "ol.shvedov@gmail.com"
  },
  "homepage": "https://github.com/shvedovskiy/redux-universal-tic-tac-toe#readme",
